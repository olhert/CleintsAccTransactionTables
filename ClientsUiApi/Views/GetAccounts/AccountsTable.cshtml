@page
@using ClientsUiApi
@model System.Threading.Tasks.Task<System.Collections.Generic.IEnumerable<ClientsUiApi.Account>>

@{
    ViewBag.Title = "AccountsTable";
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Accounts</title>
</head>
<body>
<div>
    <table>
        <tr>
            <th>Account Id</th>
            <th>Client Id</th>
            <th>Currency</th>
            <th>Balance</th>
        </tr>
        
        @foreach (Account item in ViewBag.Accounts)
        {
            <tr>
                <td>@item.AccountId</td>
                <td>@item.ClientId</td>
                <td>@item.Currency</td>
                <td>@item.Balance</td>
            </tr>
        }
        
    </table>
</div>
</body>
</html>
